<?php
$table = new RM_View_Table();
$table->setEditRoute('admin-tag-edit')
    ->setStatusUrl($this->ajaxUrl)
    ->setHead('Tag name')
    ->addPaginator($this->tags);
foreach ($this->tags as $tag) :
    /* @var Application_Model_Tag $tag */
    $row = $table->addRecord($tag->getId(), $tag->getContent()->getName())
        ->setDeletable();
endforeach;
echo $table;